@model Blog
<form asp-action="Create" method="post" enctype="multipart/form-data">
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Title</label>
        <input asp-for="Title" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        <span asp-validation-for="Title"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Description</label>
        <textarea asp-for="Description" class="form-control"></textarea>
        <span asp-validation-for="Description"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">RedirectUrl</label>
        <input  asp-for="RedirectUrl" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        <span asp-validation-for="RedirectUrl"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">ImageFile</label>
        <input type="file" asp-for="ImageFile"  class="form-control" id="exampleInputPassword1">
        <span asp-validation-for="ImageFile"></span>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>